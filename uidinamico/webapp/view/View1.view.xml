<mvc:View controllerName="com.sap.uidinamico.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:micro="sap.suite.ui.microchart"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
       <GenericTile id="_IDGenGenericTile1" header="Argentina" subheader="Buenos Aires">
		<tileContent>
			<TileContent id="_IDGenTileContent1">
				<content>
					<micro:HarveyBallMicroChart id="_IDGenHarveyBallMicroChart1" showTotal="false" total="12" size="Responsive" totalScale="Hr">
						<micro:items>
							<micro:HarveyBallMicroChartItem id="_IDGenHarveyBallMicroChartItem1"  fraction="{/hora}"  color="Good" fractionScale="Hr"/>
						</micro:items>
					</micro:HarveyBallMicroChart>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>

	<GenericTile id="_IDGenGenericTile2" header="Argentina" subheader="Buenos Aires">
		<tileContent>
			<TileContent id="_IDGenTileContent2">
				<content>
					<micro:HarveyBallMicroChart id="_IDGenHarveyBallMicroChart2" showTotal="false" total="12" size="Responsive" totalScale="Hr">
						<micro:items>
							<micro:HarveyBallMicroChartItem id="_IDGenHarveyBallMicroChartItem2" fraction="{/AddIntegerResult}"  color="Good" fractionScale="Hr"/>
						</micro:items>
					</micro:HarveyBallMicroChart>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>

	<GenericTile id="_IDGenGenericTile3" header="Argentina" subheader="Buenos Aires">
		<tileContent>
			<TileContent id="_IDGenTileContent3">
				<content>
					<micro:HarveyBallMicroChart id="_IDGenHarveyBallMicroChart3" showTotal="false" total="12" size="Responsive" totalScale="Hr">
						<micro:items>
							<micro:HarveyBallMicroChartItem id="_IDGenHarveyBallMicroChartItem3" fractionLabel="{/desc}" fraction="{/hora}"  color="Good" fractionScale="Hr"/>
						</micro:items>
					</micro:HarveyBallMicroChart>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>

    </Page>
</mvc:View>
